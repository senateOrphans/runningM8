

<hr>
<% @select_tracker_for_current_week = Tracker.where(settings_id: @current_marathon_settings_id, week: @training_week_number_history) %>
<%= @select_tracker_for_current_week_json = JSON.parse(@select_tracker_for_current_week.to_json) %>
<hr>
<%= @select_tracker_for_current_week_json[0]["distance_mon"] %>
<hr>
<h1>You have run <%= @total_distance_run %> miles already!</h1>
<hr>
<p>Want to <a href=/logout>log-out?</a>


<!-- <table style="width:100%"> -->
<form action="/tracker/dashboard" method="post">
  <input type="hidden" name="settings_id" value="settings_id">
<!-- NEED TO CREATE @VARIABLE for settings_id -->
  <p>

<p><input type ="hidden" name="week" value=<%="\"#{@training_week_number_history}\""%>></p>

  <!-- NEED TO FIGURE OUT HOW WEEK WILL BE FILLED IN -->
<!-- Note: Week 18 will be the Saturday or Sunday that contains the marathon date and then back out from that. -->
<h2>You are viewing last week's data!</h2>


<% @weeks_ago = 0 %>
<h2>Weeks ago: <%= @weeks_ago %></h2>
  <p><%= @duplicate_message %>
    <% give_training_week_number(@days_until_formatted, @weeks_ago) %>

<table>


  <tr>
    <td><%= (@trainingStart + (@training_week_number_history - 1) * 7).strftime("%A")  %>Monday, <%= (@trainingStart + (@training_week_number_history - 1) * 7).strftime("%m/%d")  %></td>
    <td>Tuesday, <%= (@trainingStart + (@training_week_number_history - 1) * 7 + 1).strftime("%m/%d")  %></td>
