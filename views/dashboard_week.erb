    <td>Friday, <%= (@trainingStart + (@training_week_number_history - 1) * 7 + 4).strftime("%m/%d")  %></td>
    <td>Saturday, <%= (@trainingStart + (@training_week_number_history - 1) * 7 + 5).strftime("%m/%d")  %></td>
    <td>Sunday, <%= (@trainingStart + (@training_week_number_history - 1) * 7 + 6).strftime("%m/%d")  %></td>
  </tr>
  <tr>
    <td><input type="number" name="distance_mon"></td>
    <td><input type="number" name="distance_tues"></td>
    <td><input type="number" name="distance_wed"></td>
    <td><input type="number" name="distance_thurs"></td>
    <td><input type="number" name="distance_fri"></td>
    <td><input type="number" name="distance_sat"></td>
    <td><input type="number" name="distance_sun"></td>
  </tr>
<tr>
  <td><%= @novice[@training_week_number_history][0] %></td>
  <td><%= @novice[@training_week_number_history][1] %></td>
  <td><%= @novice[@training_week_number_history][2] %></td>
  <td><%= @novice[@training_week_number_history][3] %></td>
  <td><%= @novice[@training_week_number_history][4] %></td>
  <td><%= @novice[@training_week_number_history][5] %></td>
  <td><%= @novice[@training_week_number_history][6] %></td>
</tr>
<tr>
  <td><%=@select_tracker_for_current_week_json[0]["distance_mon"].to_i%></td>
  <td><%=@select_tracker_for_current_week_json[0]["distance_tues"].to_i%></td>
  <td><%=@select_tracker_for_current_week_json[0]["distance_wed"].to_i%></td>
  <td><%=@select_tracker_for_current_week_json[0]["distance_thurs"].to_i%></td>
  <td><%=@select_tracker_for_current_week_json[0]["distance_fri"].to_i%></td>
  <td><%=@select_tracker_for_current_week_json[0]["distance_sat"].to_i%></td>
  <td><%=@select_tracker_for_current_week_json[0]["distance_sun"].to_i%></td>
  <tr>
</table>

<input type="submit" value="Submit">
</form>
